import time
import pyautogui

# delay to switch windows
time.sleep(10)

# message to spam
message = "Hello stranger!"

# loop to spam
while True:
    # Fetch and type the message
    pyautogui.write(message)
    # Press enter to send the message
    pyautogui.press('enter')

    #Add a delay to simulate human typing
    time.sleep(1.0)
